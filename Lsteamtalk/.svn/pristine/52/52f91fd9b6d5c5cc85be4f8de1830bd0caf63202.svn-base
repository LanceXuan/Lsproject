<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>修改登录密码-LSTeamTalk</title>
<script type="text/javascript" src="../jqueryeasyui/jquery.min.js" charset="utf-8"></script>
<script type="text/javascript"  src="../js/json2.js" charset="utf-8"></script> 
<script type="text/javascript"  src="../js/ModifyLoginPassword.js" charset="utf-8"></script> 
<link rel="stylesheet" type="text/css" href="../css/Common2.css"/>
<link rel="stylesheet" type="text/css" href="../css/Header.css"/>
<link rel="stylesheet" type="text/css" href="../css/ModifyLoginPassword_SetNewLoginPassword.css"/>
</head>
<body>
<!--top-->
<div id="top">
	<dl>
        <dt>你好，欢迎使用LSTeamTalk！</dt>
        <dd>

        </dd>
    </dl>
</div> 

<!--header-->
<div id="header">
	<dl>
        <dt><a href="#" title="LSTeamTalk"></a></dt>
        <dd>&nbsp;</dd>
    </dl>
</div>
<!--Main-->
<div id="Main">
	<dl class="inner">
    	<dt>修改登录密码</dt>
		<dd>

            <table width="100%%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                	<th width="23%" height="55" align="right" valign="middle">当前登录密码</th>
                    <!--label需用JS控制输入内容时隐藏    <label for="PayPwd"/>输入原登录密码</label>-->
                    <td width="77%" align="left" valign="middle">
                    <div>
                    
                        <input name="PayPwd" id="textPayPwd" type="password" size="30" placeholder="输入原登录密码" />
                    </div>
                    <em  id="labelPayPwd"  class="error"></em>
                    </td>
                </tr>
                <tr>
                	<th width="23%" height="55" align="right" valign="middle">新的登录密码</th>
                    <!--label需用JS控制输入内容时隐藏<label for="PayPwd"/>输入新的登录密码</label>-->
                    <td width="77%" align="left" valign="middle">
                    <div>
                        
                        <input name="PayPwd" id="textPayNewPwd" type="password" size="30" placeholder="输入新的登录密码" />
                    </div>
                    <em id="labelPayNewPwd"  class="error"></em>
                    </td>
                </tr>
                <tr>
                	<th width="23%" height="55" align="right" valign="middle">确认登录密码</th>
                    <!--label需用JS控制输入内容时隐藏<label for="PayPwdConfirm"/>再次输入密码</label>-->
                    <td width="77%" align="left" valign="middle">
                    <div>
                        <input name="PayPwdConfirm" id="textPayPwdConfirm" type="password" size="30" placeholder="确认登录密码" />
                    </div>
                    <em id="labelPayPwdConfirm"  class="error"></em>
                    </td>
                </tr>
                <tr>
                	<td height="110">&nbsp;</td>
                    <!--当用户填写好信息后按钮才可以使用，去掉disabled="disabled"-->
                    <td align="left" valign="middle">
                    	<input name="ConfirmBtn" type="button" id="ConfirmBtn"  value="提交" />
                	</td>
            	</tr>
        	</table>
        </dd>
	</dl>
</div>
</body>
</html>